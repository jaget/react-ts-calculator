{
    "singleQuote": false,
    "jsxBracketSameLine": false,
    "useTabs": true,
    "eslintIntegration": false,
    "tslintIntegration": true,
    "requireConfig": false,
    "stylelintIntegration": false,
    "arrowParens": "always",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": true,
    "tsxSingleQuote": true,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "tabWidth": 4,
    "trailingComma": "es5"
}